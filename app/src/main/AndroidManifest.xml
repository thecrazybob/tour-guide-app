<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.abudhabitourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MallsActivity"
            android:label="@string/malls" />
        <activity
            android:name=".CulturalActivity"
            android:label="@string/cultural_places" />
        <activity
            android:name=".FamilyActivity"
            android:label="@string/family_attractions"/>
        <activity
            android:name=".IslandActivity"
            android:label="@string/islands" />
    </application>

</manifest>